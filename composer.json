{
    "name": "antonioprimera/laravel-js-localization",
    "description": "Laravel localization for JS frontends.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AntonioPrimera\\LaravelJsLocalization\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Antonio Primera",
            "email": "antonio@cus.ro"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.1",
        "illuminate/console": ">=11.0",
        "antonioprimera/filesystem": "^v1.6|v2.2",
        "illuminate/support": ">=11.0",
        "illuminate/http": ">=11.0"
    },
    "require-dev": {
        "laravel/prompts": "^0.1.0@dev"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AntonioPrimera\\LaravelJsLocalization\\Providers\\LaravelJsLocalizationServiceProvider"
            ],
            "aliases": {
                "LocaleManager": "AntonioPrimera\\LaravelJsLocalization\\Facades\\LocaleManager"
            }
        }
    }
}
